package multiDimensionalArrayCheck;

public class CheckArray {
	
	public void findElement(String array[][], int rowLength, int colLength, String element)
	{
		for(int i=0; i< rowLength; i++)
		{
			for(int j=0; j< colLength; j++)
			{
				if(element.equals(array[rowLength][colLength]))
				{
					System.out.println(element+"Is Found At Position"+rowLength+"and"+colLength);
				}
				
				
			}
		}
	}

}
