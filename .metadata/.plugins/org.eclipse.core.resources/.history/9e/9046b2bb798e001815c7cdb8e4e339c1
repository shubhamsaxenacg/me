package TrainingAssignment;

public class TrainingCost {
	public static void main(String args[])
	{
		Training trainingP = new PublicTraining("Java", 5000, 50);
		System.out.println("Cost of Public training is : "+trainingP.getOrderValue());
		
		Training trainingC = new CorporateTraining("Bigdata", 35000, 4);
		System.out.println("Cost of Corporate training is : "+trainingC.getOrderValue());
	}
}
