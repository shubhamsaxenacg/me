package question1;

public class Second extends First{
	
	int a2;
	int b2;
	
	Second()
	{
		super();
		
		System.out.println("Second Class");
	}

	
	public void display()
	{
		super.display();
		System.out.println("Displaying Second class...");
	}
	
	public void displayNew()
	{
		System.out.println("Displaying Second class new...");
	}

}
