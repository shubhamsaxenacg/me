import java.util.Scanner;

public class TenInput {
	public static void main(String args[]){
		Scanner sc = new Scanner(System.in);
		boolean checkEqual;
		int j=0;
		for(int i=0; i<10; i++) {
			System.out.print("Enter string: ");
			String stringCheck = sc.next();
		
			String reverse = new StringBuffer(stringCheck).reverse().toString();
			
			checkEqual = reverse.equals(stringCheck);
			
			if(checkEqual)
			{
				
					String stringToCheck[] = stringCheck;
				
				
			}
			else
			{
				System.out.println("Not Palindrome");

			}
		}
		for(int i=0; i<stringToCheck.length; i++) {
			System.out.println( stringToCheck[i]);
		}
		
	}
}
